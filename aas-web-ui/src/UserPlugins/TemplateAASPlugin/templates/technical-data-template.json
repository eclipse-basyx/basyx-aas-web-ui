{
    "id": "technical-data-template",
    "name": "Technical Data Template",
    "description": "[JSON] Template for technical product data following ZVEI specification",
    "version": "1.2",
    "aasVersion": "3.0",
    "category": "Technical",
    "tags": ["technical", "product", "specifications", "ZVEI"],
    "defaults": {
        "idShort": "TechnicalDataAAS",
        "semanticId": "https://admin-shell.io/ZVEI/TechnicalData/1/2"
    },
    "submodels": [
        {
            "id": "technical-data",
            "name": "TechnicalData",
            "semanticId": "https://admin-shell.io/ZVEI/TechnicalData/1/2",
            "version": "1.2",
            "required": true,
            "elements": [
                {
                    "idShort": "GeneralInformation",
                    "type": "SubmodelElementCollection",
                    "required": true,
                    "description": "General information about the product",
                    "semanticId": "https://admin-shell.io/ZVEI/TechnicalData/GeneralInformation/1/1",
                    "children": [
                        {
                            "idShort": "ProductImage",
                            "type": "File",
                            "valueType": "image/*",
                            "required": false,
                            "description": "Product image file",
                            "semanticId": "https://admin-shell.io/ZVEI/TechnicalData/ProductImage/1/1"
                        },
                        {
                            "idShort": "ProductName",
                            "type": "MultiLanguageProperty",
                            "required": true,
                            "description": "Name of the product"
                        },
                        {
                            "idShort": "SerialNumber",
                            "type": "Property",
                            "valueType": "xs:string",
                            "required": true,
                            "description": "Serial number of the product"
                        },
                        {
                            "idShort": "Manufacturer",
                            "type": "MultiLanguageProperty",
                            "required": true,
                            "description": "Name of the manufacturer"
                        }
                    ]
                },
                {
                    "idShort": "TechnicalProperties",
                    "type": "SubmodelElementCollection",
                    "required": true,
                    "description": "Technical properties of the product",
                    "children": [
                        {
                            "idShort": "Weight",
                            "type": "Property",
                            "valueType": "xs:double",
                            "required": true,
                            "description": "Weight of the product in kilograms"
                        },
                        {
                            "idShort": "Dimensions",
                            "type": "SubmodelElementCollection",
                            "required": true,
                            "description": "Physical dimensions of the product",
                            "children": [
                                {
                                    "idShort": "Length",
                                    "type": "Property",
                                    "valueType": "xs:double",
                                    "required": true,
                                    "description": "Length in millimeters"
                                },
                                {
                                    "idShort": "Width",
                                    "type": "Property",
                                    "valueType": "xs:double",
                                    "required": true,
                                    "description": "Width in millimeters"
                                },
                                {
                                    "idShort": "Height",
                                    "type": "Property",
                                    "valueType": "xs:double",
                                    "required": true,
                                    "description": "Height in millimeters"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
} 