{
  "name": "aas-web-ui",
  "version": "3.0.0",
  "description": "AAS Web UI",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "prebuild": "cross-env NODE_ENV=production yarn lint:check && vitest run && vue-tsc --noEmit",
    "build": "vite build",
    "preview": "vite preview",
    "lint:check": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "postinstall": "mkdir -p public/wasm && cp node_modules/@thatopen/fragments/dist/Worker/worker.mjs public/ && cp node_modules/web-ifc/*.wasm public/wasm/"
  },
