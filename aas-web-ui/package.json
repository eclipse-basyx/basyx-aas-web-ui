    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.48.1",
    "@thatopen/ui-obc": "^3.3.1",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "apexcharts": "^5.3.6",
    "i18n-iso-countries": "^7.14.0",
    "iso-639-1": "^3.1.5",
    "js-yaml": "^4.1.1",
    "leaflet": "^1.9.4",
    "md5": "^2.3.0",
    "mermaid": "^11.12.2",
